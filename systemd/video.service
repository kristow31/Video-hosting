[Unit]
Description=Video service my
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/home/ubuntu/code/Video-hosting/src/
Environment="PATH=/home/ubuntu/code/Video-hosting/env/bin"
ExecStart=/home/ubuntu/code/Video-hosting/env/bin/gunicorn -w 4 -k uvicorn.workers.UvicornWorker main:app

[Install]
WantedBy=multi-user.target